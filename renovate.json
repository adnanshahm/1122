{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends":[
    "config:recommended"
  ],
      "packageRules": [
        {
            "description": "Only Update package.json",
            "matchFileNames": ["Dockerfile"]
        },
        {   
            "description": "Disable updates for dockerfiles.",
            "matchManagers": ["docker-compose", "dockerfile"],
            "enabled": false
        }
      ]
}
